{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-de-dev-southindia-001"
		},
		"LS_ASQL_employeeDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_ASQL_employeeDB'"
		},
		"LS_onprem_dept_emp_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_onprem_dept_emp'"
		},
		"LS_onprem_sqlDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_onprem_sqlDB'",
			"defaultValue": "Integrated Security=False;Data Source=ASUS;Initial Catalog=Employee;User ID=sudhir"
		},
		"LS_AKV_onprem_sqlDB_password_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-sudhir.vault.azure.net/"
		},
		"ls_kv_employeeDB_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-sudhir.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-de-dev-southindia-001_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ASQL_employeeDB_connectionString": {
						"value": "[parameters('LS_ASQL_employeeDB_connectionString')]"
					},
					"LS_onprem_dept_emp_connectionString": {
						"value": "[parameters('LS_onprem_dept_emp_connectionString')]"
					},
					"LS_onprem_sqlDB_connectionString": {
						"value": "[parameters('LS_onprem_sqlDB_connectionString')]"
					},
					"LS_AKV_onprem_sqlDB_password_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_AKV_onprem_sqlDB_password_properties_typeProperties_baseUrl')]"
					},
					"ls_kv_employeeDB_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_kv_employeeDB_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}